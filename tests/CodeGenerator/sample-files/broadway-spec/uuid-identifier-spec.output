<?php

namespace spec\SomeNamespace;

use PhpSpec\ObjectBehavior;
use Prophecy\Argument;
use Ramsey\Uuid\Uuid;
use SomeNamespace\SomeClass;
class SomeClassSpec extends ObjectBehavior
{
